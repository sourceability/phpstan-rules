{
    "name": "sourceability/phpstan-rules",
    "description": "PHPStan additional rules",
    "autoload": {
        "psr-4": {
            "Sourceability\\PHPStan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sourceability\\PHPStan\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "phpstan/phpstan": "^0.12 || ^1.0",
        "phpstan/phpstan-strict-rules": "^0.12 || ^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "type": "library",
    "license": "MIT"
}
